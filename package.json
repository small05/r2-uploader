    "autoprefixer": "^10.4.15",
    "axios": "^1.5.0",
    "bun-types": "^1.0.1",
    "compressorjs": "^1.2.1",
    "crypto-js": "^4.1.1",
    "marked": "^9.0.0",
    "mongodb": "^6.0.0",
    "nanoid": "^4.0.2",
    "pinia": "^2.1.6",
    "postcss": "^8.4.29",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0"
  }
}
